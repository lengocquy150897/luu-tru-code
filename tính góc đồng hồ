# t-i-li-u-t-p-code
lưu lại những gì đã học code
#include<stdio.h>

main() {
	int gio, phut;
	float doGio, doPhut;
	
	printf("Nhap so gio, phut: ");
	scanf("%d %d", &gio, &phut);
	
	doGio = (gio*60 + phut) * 0.5;
	doPhut = phut * 6;
	
	printf("So do kim gio la : %f \nSo do kim phut la: %f", doGio, doPhut);
}
